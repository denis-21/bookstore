<!DOCTYPE html>
<html>
<head>
  <title>Bookstore</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= render 'layouts/header' %>

<div class="container">
  <div class="row">
    <div class="col-sm-11">
      <%= render 'layouts/msg' %>

      <%= yield %>

    </div>
  </div>

</div>


<footer class="footer"></footer>
</body>
</html>
